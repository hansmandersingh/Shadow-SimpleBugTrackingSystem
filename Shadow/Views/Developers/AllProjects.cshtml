@model List<Shadow.Models.Project>
@{
    ViewBag.Title = "AllProjectsByUser";
}

<h2>All Projects</h2>
@Html.ActionLink("View Notification", "AllNotification") 
@if (Model != null)
{
    <ul>
        @foreach (var pro in Model)
        {
            <li>
                @pro.Name
            </li>
        }
    </ul>
}

